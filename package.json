{
  "name": "logical-rule-computation",
  "version": "1.0.2",
  "description": "基于JavaScript实现的逻辑规则计算库，可以通过添加逻辑和对比条件来快速计算数据是否符合要求",
  "author": "Rv_Jiang <rv_jiang@qq.com>",
  "keywords": [
    "js",
    "JavaScript",
    "logical",
    "rule",
    "computation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CwRv07/logical-rule-computation.git"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/es/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "start": "npm run build && npm run init:vue && npm run dev:vue",
    "dev": "npm run dev:vue",
    "build": "tsc && tsc-alias && vite build",
    "init:vue": "cd ./test/vue3 && npm i",
    "dev:vue": "vite -c ./test/vue3/vite.config.ts",
    "build:vue": "cd ./test/vue3 && npm run build",
    "deploy:vue": "cd ./test/vue3 && npm i && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}
